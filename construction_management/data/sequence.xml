<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_construction_kirim" model="ir.sequence">
            <field name="name">Kirim</field>
            <field name="code">construction.kirim</field>
            <field name="prefix">KIRIM/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_construction_daily_photo" model="ir.sequence">
            <field name="name">Daily Photo</field>
            <field name="code">construction.daily.photo</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_construction_material_request_batch" model="ir.sequence">
            <field name="name">Material Request Batch</field>
            <field name="code">construction.material.request.batch</field>
            <field name="prefix">MRB/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
